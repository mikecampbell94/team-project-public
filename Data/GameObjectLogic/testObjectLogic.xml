  <ReceiveMessage type="CollisionMessage">
    <Action>
      <Condition type="AND">
        <objectIdentifier>var</objectIdentifier>
        <colliderIdentifier>floor</colliderIdentifier>
      </Condition>

      <SendMessage type="PAINT_TRAIL_FOR_GAMEOBJECT">
        <destination>RenderingSystem</destination>
        <resource>var</resource>
      </SendMessage>
    </Action>
  </ReceiveMessage>
	
	
	<Timed interval="2.0">
		<SendMessage type="APPLY_RANDOM_IMPULSE">
			<destination>Physics</destination>
			<resource>var</resource>
		    <impulse>
				<random>
					<min>-10</min>
					<max>10</max>
				</random>
				<random>
					<min>0</min>
					<max>1</max>
				</random>
				<random>
					<min>-10</min>
					<max>10</max>
				</random>
			</impulse>
		</SendMessage>
	</Timed>
    
    
</GameplayScript>

