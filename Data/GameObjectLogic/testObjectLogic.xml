<GameplayScript>
  <ReceiveMessage type="CollisionMessage">
    <Action>

      <Condition type="AND">
        <objectIdentifier>var</objectIdentifier>
        <colliderIdentifier>floor</colliderIdentifier>
      </Condition>

	  <!--
