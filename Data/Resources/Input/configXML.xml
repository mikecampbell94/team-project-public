<Config inputType="Keyboard">
		<KeyMapping name="moveForward">
			<KeyAssignedTo>KEYBOARD_I</KeyAssignedTo>
			<Actions>
				<Move>
					<X>000</X>
					<Y>000</Y>
					<Z>50</Z>
				</Move>
			</Actions>
		</KeyMapping>
		<KeyMapping name="moveLeft">
			<KeyAssignedTo>KEYBOARD_J</KeyAssignedTo>
			<Actions>
				<Move>
					<X>50</X>
					<Y>000</Y>
					<Z>000</Z>
				</Move>
			</Actions>
		</KeyMapping>
		<KeyMapping name="moveBackwards">
			<KeyAssignedTo>KEYBOARD_K</KeyAssignedTo>
			<Actions>
				<Move>
					<X>000</X>
					<Y>000</Y>
					<Z>-50</Z>
				</Move>
			</Actions>
		</KeyMapping>
		<KeyMapping name="moveRight">
			<KeyAssignedTo>KEYBOARD_L</KeyAssignedTo>
			<Actions>
				<Move>
					<X>-50</X>
					<Y>000</Y>
					<Z>000</Z>
				</Move>
			</Actions>
		</KeyMapping>
		<KeyMapping name="jump">
			<KeyAssignedTo>KEYBOARD_SPACE</KeyAssignedTo>
			<Actions>
				<Impulse>
					<X>0</X>
					<Y>20</Y>
					<Z>0</Z>
				</Impulse>
			</Actions>
		</KeyMapping>
</Config>